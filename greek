#!/bin/zsh

# Author: James Batman
# Date Created: 12/09/2021
# Last Modified: 12/09/2021

# Description
# print greek alphabet

# Usage
# greek

clear;

echo "
$gold              Α	α $pink       alpha      $red a
$gold              Β	β $pink       beta       $red b
$gold              Γ	γ $pink       gamma      $red g
$gold              Δ	δ $pink       delta      $red d
$gold              Ε	ε $pink       epsilon    $red e
$gold              Ζ	ζ $pink       zêta       $red z
$gold              Η	η $pink       êta        $red ê
$gold              Θ	θ $pink       thêta      $red th
$gold              Ι	ι $pink       iota       $red i
$gold              Κ	κ $pink       kappa      $red k
$gold              Λ	λ $pink       lambda     $red l
$gold              Μ	μ $pink       mu         $red m
$gold              Ν	ν $pink       nu         $red n
$gold              Ξ	ξ $pink       xi         $red ks
$gold              Ο	ο $pink       omikron    $red o
$gold              Π	π $pink       pi         $red p
$gold              Ρ	ρ $pink       rho        $red r
$gold              Σ	σ $pink       sigma      $red s
$gold              Τ	τ $pink       tau        $red t
$gold              Υ	υ $pink       upsilon    $red u
$gold              Φ	φ $pink       phi        $red f
$gold              Χ	χ $pink       chi        $red ch
$gold              Ψ	ψ $pink       psi        $red ps
$gold              Ω	ω $pink       omega      $red ô
"
exit 0
