#!/bin/zsh

echo "$aqua
    create or replace TABLE VENTACITY.SBG.SBG_STATE_TRANSFER (
        SITEUUID VARCHAR(16777216) NOT NULL,
        DEVICEUUID VARCHAR(16777216) NOT NULL,
        STAMP TIMESTAMP_TZ(9) NOT NULL,
        DEVICE_TYPE VARCHAR(16777216) NOT NULL,
        DEVICE_ID NUMBER(38,0) NOT NULL,
        DATA VARIANT,
        DATECREATED TIMESTAMP_TZ(9) NOT NULL,
        SERVER_SOURCE VARCHAR(16777216) NOT NULL
    );
"

exit 0
