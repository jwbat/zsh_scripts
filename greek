#!/bin/zsh

# Author: James Batman
# Date Created: 12/09/2021
# Last Modified: 12/09/2021

# Description
# print greek alphabet

# Usage
# greek

c="$(tput setaf 200)"


echo "$c
              Α	α       alpha       a
              Β	β       beta        b
              Γ	γ       gamma       g
              Δ	δ       delta       d
              Ε	ε       epsilon     e
              Ζ	ζ       zêta        z
              Η	η       êta         ê
              Θ	θ       thêta      th
              Ι	ι       iota        i
              Κ	κ       kappa       k
              Λ	λ       lambda      l
              Μ	μ       mu          m
              Ν	ν       nu          n
              Ξ	ξ       xi         ks
              Ο	ο       omikron     o
              Π	π       pi          p
              Ρ	ρ       rho         r
              Σ	σ       sigma       s
              Τ	τ       tau         t
              Υ	υ       upsilon     u
              Φ	φ       phi         f
              Χ	χ       chi        ch
              Ψ	ψ       psi        ps
              Ω	ω       omega       ô
"
exit 0
